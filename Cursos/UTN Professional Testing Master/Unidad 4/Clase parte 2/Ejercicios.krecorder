<tr><td>if</td><td>"${Alumno}"=="si"<datalist><option></option><option>"</option><option>"$</option><option>"${</option><option>"${Alumno</option><option>"${Alumno}</option><option>"${Alumno}"</option><option>"${Alumno}"==</option><option>"${Alumno}"=="</option><option>"${Alumno}"=="si</option><option>"${Alumno}"=="si"</option></datalist></td><td></td></tr>
