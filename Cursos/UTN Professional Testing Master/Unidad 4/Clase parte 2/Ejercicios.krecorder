<tr><td>endIf</td><td><datalist><option></option></datalist></td><td></td></tr>
<tr><td>click</td><td>id=calcular<datalist><option>id=calcular</option><option>xpath=//button[@id='calcular']</option><option>xpath=(.//*[normalize-space(text()) and normalize-space(.)='Monto ($)'])[1]/following::button[1]</option><option>xpath=(.//*[normalize-space(text()) and normalize-space(.)='David Lopez'])[1]/preceding::button[1]</option><option>xpath=//*/text()[normalize-space(.)='Calcular']/parent::*</option><option>xpath=//button</option><option>css=#calcular</option></datalist></td><td></td></tr>
